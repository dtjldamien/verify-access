{
  "name": "verify-access",
  "version": "1.0.0",
  "description": "Verify Access",
  "scripts": {
    "setup": "concurrently \"npm run install:verify-access-node\" \"npm run install:verify-access-vue\" \"npm run install:mock-verify\"",
    "install:verify-access-node": "cd verify-access-node && npm install -f",
    "install:verify-access-vue": "cd verify-access-vue && npm install -f",
    "install:mock-verify": "cd mock-verify && npm install -f",
	"build": "npm run build:verify-access-vue",
	"build:verify-access-vue": "cd verify-access-vue && npm run build",
    "start": "concurrently \"npm run start:verify-access-node\" \"npm run start:verify-access-vue\" \"npm run start:mock-verify\"",
    "start:verify-access-node": "cd verify-access-node && npm start",
    "start:verify-access-vue": "cd verify-access-vue && npm run serve",
    "start:mock-verify": "cd mock-verify && npm start"
  },
  "dependencies": {
    "concurrently": "^6.2.0"
  },
  "author": "Damien Tan <dtjldamien@gmail.com>",
  "license": "ISC"
}
