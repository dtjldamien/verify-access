{
  "name": "verify-access",
  "version": "1.0.0",
  "description": "Verify Access",
  "scripts": {
    "install": "concurrently \"yarn install:verify-access-node\" \"yarn install:verify-access-vue\" \"yarn install:mock-verify\"",
    "install:verify-access-node": "cd verify-access-node && yarn",
    "install:verify-access-vue": "cd verify-access-vue && yarn",
    "install:mock-verify": "cd mock-verify && yarn",
    "start": "concurrently \"yarn start:verify-access-node\" \"yarn start:verify-access-vue\" \"yarn start:mock-verify\"",
    "start:verify-access-node": "cd verify-access-node && yarn start",
    "start:verify-access-vue": "cd verify-access-vue && yarn serve",
    "start:mock-verify": "cd mock-verify && yarn start"
  },
  "dependencies": {
    "concurrently": "^6.2.0",
    "dotenv": "^10.0.0"
  },
  "author": "Damien Tan <dtjldamien@gmail.com>",
  "license": "ISC"
}
